<div id="<%= dom_id coin %>">

<p><strong><%= nome_aplicacao %></strong></p>
  <p>
    <strong>Descrição:</strong>
    <%= coin.discription %>
  </p>

  <p>
    <strong>Sigla:</strong>
    <%= coin.acronym %>
  </p>

  <p>
    <strong>Imagem:</strong>
    <%= image_tag coin.url_image, width:25, height:25 %>
  </p>

</div>
